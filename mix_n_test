      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "Merge datasets"
      ],
      "metadata": {
        "id": "jbb7n4C6ctk9"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "import shutil\n",
        "\n",
        "# Define source and target directories\n",
        "source_dirs = ['/content/real_data', '/content/render_data']\n",
        "target_dir = '/content/merged_data'\n",
        "image_out = os.path.join(target_dir, 'images')\n",
        "label_out = os.path.join(target_dir, 'labels')\n",
        "\n",
        "# Create output directories\n",
        "os.makedirs(image_out, exist_ok=True)\n",
        "os.makedirs(label_out, exist_ok=True)\n",
        "\n",
        "# Function to copy files with overwrite warning\n",
        "def copy_all_files(src_dir, dst_dir):\n",
        "    for filename in os.listdir(src_dir):\n",
        "        src_path = os.path.join(src_dir, filename)\n",
        "        dst_path = os.path.join(dst_dir, filename)\n",
        "\n",
        "        # Optional: Rename if duplicate\n",
        "        if os.path.exists(dst_path):\n",
        "            name, ext = os.path.splitext(filename)\n",
        "            counter = 1\n",
        "            while os.path.exists(dst_path):\n",
        "                new_filename = f\"{name}_{counter}{ext}\"\n",
        "                dst_path = os.path.join(dst_dir, new_filename)\n",
        "                counter += 1\n",
        "\n",
        "        shutil.copy(src_path, dst_path)\n",
        "\n",
        "# Copy from both datasets\n",
        "for source in source_dirs:\n",
        "    copy_all_files(os.path.join(source, 'images'), image_out)\n",
        "    copy_all_files(os.path.join(source, 'labels'), label_out)\n",
        "\n",
        "print(\"Datasets merged successfully into 'MergedDataset'.\")\n"
      ],
      "metadata": {
        "id": "CWxv3_5mcw6i",
        "outputId": "fcef6564-267c-4f60-fa06-52817fa72ec4",
        "colab": {
          "base_uri": "https://localhost:8080/"
        }
      },
      "execution_count": 7,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Datasets merged successfully into 'MergedDataset'.\n"
          ]
        }
      ]
    },
